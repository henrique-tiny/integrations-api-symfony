{
    "listeners": {
        "*:9004": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "uri": [
                    "*.php",
                    "*.php/*"
                ]
            },

            "action": {
                "pass": "applications/symfony/direct"
            }
        },
        {
            "action": {
                "share": "/var/www/public$uri",
                "fallback": {
                    "pass": "applications/symfony/index"
                }
            }
        }
    ],
    "applications": {
        "symfony": {
            "type": "php",
            "targets": {
                "direct": {
                    "root": "/var/www/public/"
                },

                "index": {
                    "root": "/var/www/public/",
                    "script": "index.php"
                }
            },
            "processes": {
                "max": 50,
                "spare": 50
            }
        }
    }
}